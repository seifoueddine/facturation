<!--- Activity sidebar --->
<aside id="activity-sidebar" class='<%= "#{ 'side-show' if side_nav_opened?}"%>'>
  <span class="btn-activity"><i class="material-icons">
    keyboard_arrow_left
  </i></span>
  <div class="activity-scroll-holder scrollContainer">
    <!--- activities list--->
    <div class="status-info" id="terms_listing"><%= render partial: 'payment_terms/payment_terms_settings' %></div>
    <div class="status-info" id="recurring_listing"><%= render partial: 'recurring_frequencies/recurring_frequencies_settings' %></div>
    <div class="status-info active" id="users_listing"><%= render partial: 'sub_users/settings_listing' %></div>
    <div class="status-info" id="companies_listing"><%= render partial: 'companies/settings_listing' %></div>
    <div class="status-info" id="roles_listing"><%= render partial: 'roles/roles_settings' %></div>
    <div class="status-info" id="client_roles_listing"><%= render partial: 'roles/client_roles_settings' %></div>
    <%#= render partial: 'roles/settings_roles_listing' %>
  </div>
</aside>
