<% if @company_updated.eql?(true) %>
swal({
    icon: "success",
    title: 'Success',
    buttons: false,
    timer: 3000,
    text: "<%= t('views.companies.updated_msg') %>"}).then(function(){
    window.location = '<%= companies_path %>'
});
<% else %>
swal({
    icon: "error",
    title: 'Oops..!',
    buttons: false,
    timer: 3000,
    text: "<%= @company.errors.full_messages.join('.') %>"
});
<% end %>